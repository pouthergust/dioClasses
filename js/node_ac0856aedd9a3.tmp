//Tornando um objeto iteravel
const obj = { 
    values: [1, 2, 3, 4],
    [Symbol.iterator]() {
        let i = 0;
        return {
            next: () => {
                return {
                    value: this.values[i],
                    done: i > this.values.length
                };
                i++;
            }
        };
    }
};

for (let value of obj) {
    console.log(value);
};